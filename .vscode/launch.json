    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Debug Game",
            "type": "gdb",
            "request": "launch",
            "target": "./Game",
            "cwd": "${workspaceRoot}/build",
            "valuesFormatting": "parseText"
        },
        {
            "name": "Debug Dev Scene Import",
            "type": "gdb",
            "request": "launch",
            "target": "./Dev",
            "cwd": "${workspaceRoot}/build",
            "valuesFormatting": "parseText",
            "arguments": "-t SCENE_IMPORT -i PlayerCharacter/PlayerCharacter.assetsmith"
        },
        {
            "name": "Debug Dev Scene Preview",
            "type": "gdb",
            "request": "launch",
            "target": "./Dev",
            "cwd": "${workspaceRoot}/build",
            "valuesFormatting": "parseText",
            "arguments": "-t SCENE_PREVIEW -i Props/Environment/Foliages/Tree1.wiscene"
        },
        {
            "name": "Debug Dev TestMap01",
            "type": "gdb",
            "request": "launch",
            "target": "./Dev",
            "cwd": "${workspaceRoot}/build",
            "valuesFormatting": "parseText",
            "arguments": "-t SCENE_PREVIEW -i Tests/TestMap01.wiscene"
        },
    ]
}